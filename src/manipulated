          "key": "key"
        },
        "properties": {
          "key": {
            "type": "string"
          },
          "label": {
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "securitySchemes": {
      "x_auth_token": {
        "description": "HTTP header containing the User or Project API key that will be used to authenticate the request.\n",
        "in": "header",
        "name": "X-Auth-Token",
        "type": "apiKey",
        "x-displayName": "X-Auth-Token"
      }
    }
  }
}